FRAMEWORK_SEARCH_PATHS = "${PODS_ROOT}/Sparkle" "$(PODS_ROOT)/Sparkle"
GCC_PREPROCESSOR_DEFINITIONS = $(inherited) COCOAPODS=1
HEADER_SEARCH_PATHS = "${PODS_ROOT}/Headers" "${PODS_ROOT}/Headers/MASPreferences" "${PODS_ROOT}/Headers/Sparkle"
OTHER_CFLAGS = $(inherited) -isystem "${PODS_ROOT}/Headers" -isystem "${PODS_ROOT}/Headers/MASPreferences" -isystem "${PODS_ROOT}/Headers/Sparkle"
OTHER_LDFLAGS = -ObjC -framework Sparkle
PODS_ROOT = ${SRCROOT}/Pods